@model SearchViewModel

<li>

        <form asp-controller="Product" asp-action="SearchList" class="form-inline">
            <input asp-for="Phrase" style="margin-top: 8px" class="form-control mr-sm-2" type="search" value="@Model.Phrase" placeholder="Search" aria-label="Search">
            <button style="margin-top: 8px" class="btn btn-outline-success my-2 my-sm-0" type="submit">Search</button>
        </form>

</li>
